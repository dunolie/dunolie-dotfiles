vim: syntax=screen
caption string '%{= dd}'
msgminwait 0
bind 0 exec screenwm --meta select 0
bind 1 exec screenwm --meta select 1
bind 2 exec screenwm --meta select 2
bind 3 exec screenwm --meta select 3
bind 4 exec screenwm --meta select 4
bind 5 exec screenwm --meta select 5
bind 6 exec screenwm --meta select 6
bind 7 exec screenwm --meta select 7
bind 8 exec screenwm --meta select 8
bind 9 exec screenwm --meta select 9
bind ) exec screenwm layout_select 0
bind ! exec screenwm layout_select 1
bind @ exec screenwm layout_select 2
bind \# exec screenwm layout_select 3
bind $ exec screenwm layout_select 4
bind % exec screenwm layout_select 5
bind \^ exec screenwm layout_select 6
bind & exec screenwm layout_select 7
bind * exec screenwm layout_select 8
bind ( exec screenwm layout_select 9
bind j exec screenwm --meta=only down
bind ^j exec screenwm --meta=only down
bind k exec screenwm --meta=only up
bind ^k exec screenwm --meta=only up
bind h exec screenwm --meta=only left
bind ^h exec screenwm --meta=only left
bind l exec screenwm --meta=only right
bind ^l exec screenwm --meta=only right
bind ^a exec screenwm --meta other
bind c exec screenwm --meta screen
bind ^c exec screenwm --meta screen
bind d exec screenwm --meta detach
bind ^d exec screenwm --meta detach
bind e exec screenwm --meta kill
bind ^e exec screenwm --meta kill
bind i exec screenwm --meta info
bind ^i exec screenwm --meta info
bind m exec screenwm window_meta
bind ^m exec screenwm window_meta
bind n exec screenwm --meta next
bind ^n exec screenwm --meta next
bind o exec screenwm --meta other
bind p exec screenwm --meta prev
bind ^p exec screenwm --meta prev
bind r exec screenwm --meta redisplay
bind ^r exec screenwm --meta redisplay
bind C exec screenwm layout_screen
bind E exec screenwm layout_kill
bind M exec screenwm window_nometa
bind N exec screenwm layout_next
bind O exec screenwm layout_other
bind P exec screenwm layout_prev
bind Q exec screenwm --meta only
bind R exec screenwm --meta=only layout_redisplay
bind S exec screenwm --meta split
bind V exec screenwm --meta vert_split
bind X exec screenwm --meta remove
bind ^\ exec screenwm --meta quit
